{
  "scripts": {
    "server": "nodemon dist/server/main.js  --exec babel-node",
    "dev": "NODE_ENV=development concurrently \"yarn run client\" \"yarn run server\"",
    "client": "webpack-dev-server --config ./config/client/webpack.config.js --open --mode development",
    "build": "yarn run build_client && yarn run build_server",
    "build_client": "webpack --config ./config/client/webpack.config.js --mode development",
    "build_server": "webpack --config ./config/server/webpack.config.js --mode development",
    "start": "NODE_ENV=production yarn run build && yarn run server",
    "test": "npx jest --testTimeout=10000"
  },
